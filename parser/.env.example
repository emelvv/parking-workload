# 2GIS API key. Obtain it from https://dev.2gis.com/.
DGIS_API_KEY=your_api_key_here

# Default radius for parking search in meters.
DEFAULT_RADIUS=600

# Maximum number of parking records to request from the API.
DEFAULT_LIMIT=10

# Coordinates of the city center used for distance calculations (latitude, longitude).
CENTER_COORDINATES=55.7558, 37.6173

# Use heuristics to estimate parking price from distance to the center (1/0).
AUTO_PRICE_BY_DISTANCE=0

# Optional service runtime options.
# PORT=8000
# RELOAD=0
